{
  "name": "keiro",
  "description": "A filesystem routing middleware",
  "version": "0.0.1-beta1",
  "type": "module",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "lint": "eslint ./src",
    "test": "vitest --run"
  },
  "keywords": [
    "node",
    "wintergc",
    "file-system-routing",
    "routing",
    "fetch",
    "middleware"
  ],
  "author": "Neo-Ciber94",
  "license": "MIT",
  "dependencies": {
    "cookie": "^0.6.0",
    "glob": "^10.4.2",
    "radix3": "^1.1.2"
  },
  "devDependencies": {
    "@types/cookie": "^0.6.0",
    "@types/node": "^20.14.8",
    "@types/polka": "^0.5.7",
    "eslint": "9",
    "polka": "^0.5.2",
    "rimraf": "^5.0.7",
    "tsx": "^4.15.7",
    "typescript": "^5.5.2",
    "vitest": "^1.6.0"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./node": {
      "import": "./dist/handler/node.js",
      "types": "./dist/handler/node.d.ts"
    },
    "./web": {
      "import": "./dist/handler/web.js",
      "types": "./dist/handler/web.d.ts"
    },
    "./types": {
      "types": "./dist/types.d.ts"
    },
    "./matchingPattern": {
      "import": "./dist/matchingPattern.js",
      "types": "./dist/matchingPattern.d.ts"
    }
  },
  "files": [
    "./dist",
    "./src",
    "./package.json"
  ]
}